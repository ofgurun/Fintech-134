{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApiSettings": {
    "CustomersApi": "https://customers-api.azurewebsites.net",
    "IdcApi": "https://api-idc.azurewebsites.net/api/",
    "FunctionKey": "YOUR KEY HERE",
    "BearerToken": "YOUR KEY HERE",
    "KvkkTextKey": "YOUR KEY HERE",
    "KvkkSaveKey": "YOUR KEY HERE",
    "OtpGenerateKey": "YOUR KEY HERE",
    "OtpSendKey": "YOUR KEY HERE",
    "OtpVerifyKey": "YOUR KEY HERE",
    "Timeout": 30
  },
  "Session": {
    "IdleTimeout": 30,
    "CookieSecure": true
  },
  "Hsts": {
    "MaxAge": 31536000,
    "IncludeSubDomains": true,
    "Preload": true
  }
}